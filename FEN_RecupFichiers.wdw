        code : |1+
         sablier(vrai)
         
         //Code de sécurité demandé sur le périphérique Bluetooth qui reçoit le fichier
         sCodeSécurité est une chaine = "0000"
         
         sPériph est une chaine = COMBO_PériphériquesBT[COMBO_PériphériquesBT]
         
         // Connexion Bluetooth
         BTConnectePériphérique(gtaPériphBT[sPériph]:nIDPériph, sCodeSécurité) 
         
         si gnIDCnxOBEX = 0 alors
         	//Connexion OBEX
         	gnIDCnxOBEX  = OBEXConnecte(obexBluetooth, gtaPériphBT[sPériph]:sAdresseMac)	
         FIN
         
         //Un message apparaît sur l'écran du mobile pour accepter ou non le transfert de fichiers
         Info("Un message va éventuellement apparaître sur l'écran de votre mobile, pensez à le valider.")
         
         //Liste les fichiers présents dans le répertoire saisi
         sListeRépertoires est une chaine = OBEXListeFichier(gnIDCnxOBEX,SAI_RépBase)
         
         sUnRépertoire est une chaine 
         sNomFic est une chaine
         //Ajoute chacun des fichiers trouvés dans la table
         POUR TOUTE CHAINE sUnRépertoire DE sListeRépertoires SEPAREE PAR RC
         	sNomFic = AnsiVersUnicode(ExtraitChaîne(sUnRépertoire, 1, TAB),alphabetUTF8)
         	TableAjouteLigne(TABLE_Fichiers,sNomFic)
         FIN
         
         Sablier(Faux)
         
         //Message si aucun fichier n'a été trouvé 
         si TABLE_Fichiers..Occurrence = 0 alors
         	info("Aucun fichier n'a été trouvé dans le répertoire spécifié.") 
         FIN
         
