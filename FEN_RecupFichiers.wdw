     name : ListePériphBT
     internal_properties : CAAAAAgAAAAMG0FJON0fyyZbjPeWf7I3+ZNEfabexV51Qw7CgtIRda/49DLmIG+1nuD0whhTrwcoR8JZmXrQ1qj4Ln8+cDjbQeMseRJcA9fbslb+8lAvBsogRbkdp3eEx8UkV4pm5ZL+6eUZWXg8iy30MjctCcYHQHq7cCUoD9BHkBoG3phKp3tCg9Dw7hrsQILA6kuvHTRtHBJ/Z7zU/hNWkWMrHIbwaVFKTPWXZLOUTujQCbXkTM9mS9xX2FANiN8pmF94
     procedure_id : 1767869209743901801
     type_code : 14
     code : |1-
      // Résumé : Procédure qui récupère la liste des périphériques BT, mémorise leur ID et adresse Mac et remplit la combo avec le nom des périphériques
      // Syntaxe :
      //ListePériphBT ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      
      Procédure ListePériphBT()
      
      //Suppression du contenu de la combo .. au préalable
      ListeSupprimeTout(COMBO_PériphériquesBT)
      
      //Récupération des périphériques BT 
      sListePériphBT est une chaine = BTListePériphérique(btListeSimple)
      sUnPériphBT, sNomPériph sont des chaines
      stInfoPériphBT est un STPériphBT
      
      POUR TOUTE CHAINE sUnPériphBT DE sListePériphBT SEPAREE PAR RC
      	
      	//Récupération de l'ID du périphérique BT
      	stInfoPériphBT:nIDPériph = ExtraitChaîne(sUnPériphBT,1,TAB)
      	//Récupération de l'adresse Mac du périphérique BT
      	stInfoPériphBT:sAdresseMac =  ExtraitChaîne(sUnPériphBT,2,TAB)
      	//Récupération du nom du périphérique BT
      	sNomPériph = ExtraitChaîne(sUnPériphBT,3,TAB)
      	
      	//Mémorisation dans un tableau associatif des informations du périphérique BT 
      	gtaPériphBT[sNomPériph ] =  stInfoPériphBT
      	
      	//Ajout dans la combo du périphérique
      	ListeAjoute(COMBO_PériphériquesBT, ExtraitChaîne(sUnPériphBT,3,TAB))
      	
      FIN
      
      si ListeOccurrence(COMBO_PériphériquesBT) = 0  alors
      	info("Aucun périphérique Bluetooth n'a été trouvé.")
      FIN
